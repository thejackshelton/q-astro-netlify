#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/jackshelton/dev/playground/test-latest-deps-q-astro/node_modules/.pnpm/astro@5.0.2_rollup@4.29.1_typescript@5.7.2_yaml@2.6.1/node_modules/astro/node_modules:/Users/jackshelton/dev/playground/test-latest-deps-q-astro/node_modules/.pnpm/astro@5.0.2_rollup@4.29.1_typescript@5.7.2_yaml@2.6.1/node_modules:/Users/jackshelton/dev/playground/test-latest-deps-q-astro/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/jackshelton/dev/playground/test-latest-deps-q-astro/node_modules/.pnpm/astro@5.0.2_rollup@4.29.1_typescript@5.7.2_yaml@2.6.1/node_modules/astro/node_modules:/Users/jackshelton/dev/playground/test-latest-deps-q-astro/node_modules/.pnpm/astro@5.0.2_rollup@4.29.1_typescript@5.7.2_yaml@2.6.1/node_modules:/Users/jackshelton/dev/playground/test-latest-deps-q-astro/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../astro/astro.js" "$@"
else
  exec node  "$basedir/../astro/astro.js" "$@"
fi
